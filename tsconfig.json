{
    "compilerOptions": {
      "outDir": "./assets/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "ES6",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true
    }
  }